{
  "name": "e-acelera-backend",
  "version": "1.0.0",
  "description": "A backend repository for e-acelera project",
  "main": "src/index.ts",
  "scripts": {
    "start": "npx tsx src/index.ts",
    "dev": "nodemon --watch src --exec tsx src/index.ts",
    "build": "npx prisma migrate deploy",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "postinstall": "npx prisma generate",
    "content:list": "node ../scripts/content-manager.js list",
    "content:validate": "node ../scripts/content-manager.js validate",
    "content:manage": "node ../scripts/content-manager.js"
  },
  "author": "daniellemadrid, barbaraanger, aceleradora-TW/teams/turma-25",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@upstash/redis": "^1.35.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "husky": "^9.0.11",
    "jose": "^5.9.6",
    "jsonwebtoken": "^9.0.2",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@jest-mock/express": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prisma": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  }
}
